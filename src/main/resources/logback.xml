    <appender
        name="CONSOLE"
        class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <charset>UTF-8</charset>
            <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] [%-5level] %c{0} - %X{collectorId} %msg%n</pattern>
        </encoder>
    </appender>

    <root level="${log.root.level}">
        <appender-ref ref="CONSOLE" />
    </root>

</configuration>
